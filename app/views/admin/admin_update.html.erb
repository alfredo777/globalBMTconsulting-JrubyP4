<h1> Actualizar admnistrador </h1>
<div class="col-md-6">
	<%= form_tag update_admin_path(id: params[:id]) do -%>
	<%= text_field_tag 'name', @admin.name , placeholder: "Name", class: "form-control" %><br/>
	<%= email_field_tag 'email', @admin.email , placeholder: "Email", class: "form-control" %><br/>
	<%= password_field_tag 'password', '', placeholder: "Password", class: "form-control" %><br/>
	<%= submit_tag 'Crear', class: "btn btn-primary" %>
	<%end%>
</div>